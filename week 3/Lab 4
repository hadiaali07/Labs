public class lab04 {
    public static void main(String[] args) {
        int[] marks = {85, 90, 78, 88, 92};
        int sum = 0;
        // Calculate sum
        for (int i = 0; i < marks.length; i++) {
            sum = sum + marks[i];
        }
        // Calculate average
        double average = (double) sum / marks.length;
        System.out.println("Average Score = " + average);
    }
}
